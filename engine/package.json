  },
  "license": "MIT",
  "main": "./src/index.js",
  "browser": "./src/index.js",
  "types": "./src/index.d.ts",
  "files": [
    ".eslintrc.json",
    "docs/**/*.html",
    "docs/**/*.md",
    "docs/**/*.toml",
    "scriptsharp-0.8.0/ss.js",
