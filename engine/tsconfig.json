{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "composite": true,
    "strict": true,
    "jsx": "preserve",
    "importHelpers": true,
    "downlevelIteration": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost"
    ]
  },
  "include": [
    "src/index.d.ts"
  ],
  "exclude": [
    "node_modules"
  ],
  "typedocOptions": {
    "readme": "../docs/engine/engine-index.md",
    "out": "../docs/engine/static/apiref/engine",
    "gaID": "UA-107473046-3",
    "excludeExternals": true,
    "excludePrivate": true,
    "validation": {
      "invalidLink": true
    },
    "intentionallyNotExported": [
      "AltTypes",
      "AltUnits",
      "BandPass",
      "BaseEngineSetting",
      "BaseLayerSetting",
      "BaseImageSetLayerSetting",
      "BaseSpreadSheetLayerSetting",
      "BaseVoTableLayerSetting",
      "Classification",
      "ConstellationFilterInterface",
      "CoordinatesType",
      "FadeType",
      "FolderGroup",
      "FolderRefreshType",
      "FolderType",
      "ImageSetType",
      "MarkerScales",
      "PlotTypes",
      "PointScaleTypes",
      "ProjectionType",
      "RAUnits",
      "ScaleTypes",
      "SettingsInterface",
      "SolarSystemObjects",
      "Thumbnail"
    ]
  }
}
