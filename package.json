{
  "private": true,
  "name": "wwt-webgl-engine-monorepo",
  "workspaces": [
    "astro",
    "embed",
    "embed-common",
    "embed-creator",
    "engine",
    "engine-helpers",
    "engine-pinia",
    "engine-types",
    "research-app",
    "research-app-messages",
    "tests"
  ],
  "scripts": {
    "build": "yarn workspaces foreach -vt run build",
    "doc": "yarn workspaces foreach -vt run doc",
    "lint": "yarn workspaces foreach -vt run lint",
    "test": "yarn workspaces foreach -vt run test",
    "serve-creator": "concurrently \"cd embed && yarn run serve -- --port=23000\" \"cd embed-creator && yarn run serve -- --port=8080\"",
    "serve-embed": "cd embed && yarn run serve",
    "serve-research": "cd research-app && yarn run serve"
  },
  "dependencies": {
    "@types/debounce": "^1.2.0",
    "@types/express-serve-static-core": "4.17.28",
    "@types/jsdom": "^16.2.13",
    "@wwtelescope/astro": "link:astro",
    "@wwtelescope/embed": "link:embed",
    "@wwtelescope/embed-common": "link:embed-common",
    "@wwtelescope/embed-creator": "link:embed-creator",
    "@wwtelescope/engine": "link:engine",
    "@wwtelescope/engine-helpers": "link:engine-helpers",
    "@wwtelescope/engine-pinia": "link:engine-pinia",
    "@wwtelescope/engine-types": "link:engine-types",
    "@wwtelescope/research-app": "link:research-app",
    "@wwtelescope/research-app-messages": "link:research-app-messages",
    "axios": "^0.22.0",
    "debounce": "^1.2.1",
    "jsdom": "^16.5.0",
    "pako": "^1.0.11",
    "uuid": "^8.3.2",
    "vue": "^3",
    "vue-color-kit": "^1.0.5",
    "vue-select": "^4.0.0-beta.5",
    "vue3-popper": "^1.5.0"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.7.0",
    "@types/nightwatch": "^2.3.12",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-plugin-typescript": "^5.0.8",
    "@vue/cli-plugin-unit-mocha": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "2.1.0",
    "chai": "^4.2.0",
    "concat-cli": "^4.0.0",
    "concurrently": "^5.2.0",
    "copyfiles": "^2.2.0",
    "cross-env": "^7.0.2",
    "eslint": "^8.24.0",
    "eslint-plugin-vue": "^9.5.1",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "mocha-headless-chrome": "^4.0.0",
    "mutationobserver-shim": "^0.3.3",
    "nightwatch": "^2.3.9",
    "sass": "^1.26.3",
    "sass-loader": "^13.0.2",
    "typedoc": "0.23.20",
    "typescript": "~4.8.4",
    "uglify-js": "^3.9.1",
    "vue-template-compiler": "~2.6.11"
  },
  "packageManager": "yarn@3.3.0"
}
