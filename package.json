  "dependencies": {
    "@wwtelescope/astro": "file:astro",
    "@wwtelescope/engine": "file:engine",
    "@wwtelescope/engine-helpers": "file:engine-helpers",
    "@wwtelescope/engine-types": "file:engine-types",
    "@wwtelescope/engine-vuex": "file:engine-vuex"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "concat-cli": "^4.0.0",
    "eslint": "^6.8.0",
    "lerna": "^3.20.2",
    "typedoc": "^0.17.0-3",
    "typescript": "~3.8.3",
    "uglify-js": "^3.9.1"
  }
}
