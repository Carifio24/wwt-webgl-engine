{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "composite": true,
    "declaration": true,
    "esModuleInterop": true,
    "incremental": true,
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "preserveWatchOutput": true,
    "resolveJsonModule": true,
    "outDir": "dist",
    "rootDir": "src",
    "strict": true,
    "strictNullChecks": false,
    "target": "es2017",
    "types": []
  },
  "include": ["src/*"],
  "typedocOptions": {
    "readme": "../docs/engine/research-app-messages-index.md",
    "out": "../docs/engine/static/apiref/research-app-messages",
    "gaID": "UA-107473046-3",
    "excludePrivate": true,
    "validation": {
      "invalidLink": true
    }
  }
